import &StandardImport

tileSize:       100
factorSize:     22
factorSpacing:  3

primeColors: primeColors =
  1:        rgbColor #c7c9ca
  2:        rgbColor #eea22e
  3:        rgbColor #87c65f
  5:        rgbColor #66cbf0
  7:        rgbColor #8e7dba
  default:  rgbColor #e65e4c

getPrimeColor: (n) ->
  primeColors[n] ? primeColors.default
